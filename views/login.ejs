    <script>
        document.querySelector('form').onsubmit = (e) => {
            e.preventDefault();
            const F = new FormData(e.target)
            if (!/[a-z0-9._+%]+@[a-z]+\.[a-z]{2,}$/.test(F.get('email'))) {
                document.getElementById("email").classList.add('err_inp')
                document.getElementById("email").focus()
                return;
            } else {
                document.getElementById("email").classList.remove('err_inp')
            }


            e.target.submit()


        }


    </script>

